#define _CRT_SECURE_NO_WARNINGS
#define	ROW 9
#include <stdio.h>
#include <locale.h>
#include <math.h>
void main()
{
	int row, col, COL=1;
	printf("\n");
	for (row = 1; row <= ROW; ++row)
		{
		for (col = 1; col <= COL; col++)
			{
				printf("%5d", col * row);
			}
		COL += 1;
		printf("\n");
		}
	
	system("pouse");
}
