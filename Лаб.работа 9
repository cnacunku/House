#define _CRT_SECURE_NO_WARNINGS
#define	ROW 9
#include <stdio.h>
#include <locale.h>
#include <math.h>
void main()
{
	setlocale(LC_CTYPE, "RUS");
	int row, col, COL=1;
	printf("\n");
	for (row = 1; row <= ROW; ++row)
		{
		for (col = 1; col <= COL; col++)
			{
				printf("%5d", col * row);
			}
		COL += 1;
		printf("\n");
		}
	system("pouse");

	printf("\n");
	int wor, loc, LOC=1;
	for (wor = 1; wor <= ROW; ++wor)
	{
		for (loc=5; loc >= LOC; loc--)
		{
			printf("1");
		}
		LOC += 1;
		printf("\n");
	}

	int ran;
	printf("Введите произвольное число : ");
	scanf("%d", &ran);
	
}
