#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <locale.h>
int main()
{
	int col,sum,cash;
	setlocale(LC_CTYPE, "RUS");
	printf("Введите кол-во товара :  ");
	scanf("%d", &col);
	printf("Введите стоимость единицы товара :  ");
	scanf("%d", &sum);
	cash = col * sum;
	printf("Стоимость покупки, с учетом %d товаров и стоимости в %dр за 1 товар, составит : %d ", col, sum, cash);
	return 0;
}
